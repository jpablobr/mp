! #include "/etc/X11/Xresources/x11-common"
!-----------------------------------------------------------------------------
! appearance
!-----------------------------------------------------------------------------

*VT100.utf8:	1

! enable colors in native X applications
#ifdef COLOR
*customization              : -color
#endif

! use the Human cursor theme from Ubuntu
Xcursor.theme               : Human

!-----------------------------------------------------------------------------
! URxvt
!-----------------------------------------------------------------------------

! interaction
Rxvt.font                   : xft:Inconsolata-12:antialias=true
URxvt.scrollBar             : off
Rxvt.scrollstyle            : plain
URxvt.iconFile              : /usr/share/icons/Humanity/apps/48/terminal.svg
URxvt.geometry              : 84Ã—24
URxvt.borderLess            : False
! URxvt*backgroundPixmap      : /home/jpablobr/Dropbox/backgrounds/simple_linux.png
! extensions
URxvt.perl-lib              : /home/jpablobr/.urxvt/perl
URxvt.perl-ext-common       : default,matcher,searchable-scrollback,keyboard-select,url-select,clipboard,fullscreen
URxvt.urlLauncher           : /usr/bin/xdg-open
URxvt.matcher.button        : 1
URxvt.keysym.M-u            : perl:url-select:select_next
URxvt.keysym.M-Escape       : perl:keyboard-select:activate
URxvt.keysym.M-s            : perl:keyboard-select:search
URxvt.underlineURLs         : true
URxvt.keysym.M-c            : perl:clipboard:copy
URxvt.keysym.M-v            : perl:clipboard:paste
URxvt.keysym.M-C-v          : perl:clipboard:paste_escaped
urxvt*keysym.F11            : perl:fullscreen:switch
URxvt.copyCommand           : xclip -i -selection clipboard
URxvt.pasteCommand          : xclip -o -selection clipboard

!-----------------------------------------------------------------------------

! interaction
XTerm*vt100.metaSendsEscape : True
XTerm*vt100.saveLines       : 8192
XTerm*VT100.allowWindowOps: true
XTerm*VT100.allowTitleOps: true
XTerm*VT100.allowFontOps: true

! appearance
XTerm*vt100.faceName        : xft:Inconsolata-12:antialias=true
XTerm*vt100.colorMode       : True
XTerm*vt100.colorBDMode     : True
XTerm*vt100.colorULMode     : True
XTerm*vt100.showBlinkAsBold : True
XTerm*vt100.veryBoldColors  : 6
XTerm*Maximized             : true

! Keep 5K of scrollback and only reset the scroll position on keypresses.
XTerm*saveLines: 5120
XTerm*scrollKey: true
XTerm*scrollTtyOutput: false

! {{{ Quick Reference
!
! Scrolling:
!   Scroll Wheel
!   Shift-PgUp/PgDn
!
! Copy/Paste:
!   Mouse-Select        : Copy to PRIMARY
!   Shift + Mouse-Select: Copy to CLIPBOARD
!
!   Middle-Click        : Paste from PRIMARY
!   Shift + Middle-Click: Paste from CLIPBOARD
!
!   Ctrl+Shift+V        : Paste from CLIPBOARD
!   Ctrl+Alt+Shift+V    : Paste from PRIMARY
!
! }}}

! More Konsole/Yakuake-like keybindings:
*VT100.Translations: #override \
                    Shift <Key>Up:              scroll-back(1) \n\
                    Shift <Key>Down:            scroll-forw(1) \n\
                    ~Meta Shift Ctrl <Key>V:    insert-selection(CLIPBOARD) \n\
                     Meta Shift Ctrl <Key>V:    insert-selection(PRIMARY) \n\
                    ~Shift <Btn1Up>:            select-end(PRIMARY, CUT_BUFFER0) \n\
                     Shift <Btn1Up>:            select-end(CLIPBOARD, CUT_BUFFER1) \n\
                    ~Shift ~Ctrl ~Meta <Btn2Up>:insert-selection(PRIMARY, CUT_BUFFER0) \n\
                     Shift ~Ctrl ~Meta <Btn2Up>:insert-selection(CLIPBOARD, CUT_BUFFER1)

!-----------------------------------------------------------------------------
! xoria256 terminal color scheme
! http://snk.tuxfamily.org/log/xoria256-terminal-color-scheme.html
!-----------------------------------------------------------------------------

!black.normal               : xoria256.LineNr.guibg
!black.bright               : black.normal.rgb - 8
URxvt.color0                 : #121212
URxvt.color8                 : #0a0a0a

!red.normal                 : red.bright.r - 8
!red.bright                 : xoria256.Special.guifg
URxvt.color1                 : #d78787
URxvt.color9                 : #df8787

!green.normal               : green.bright.g - 8
!green.bright               : xoria256.PreProc.guifg
URxvt.color2                 : #afd787
URxvt.color10                : #afdf87

!yellow.normal              : yellow.bright.rg - 8
!yellow.bright              : xoria256.Special.guifg
URxvt.color3                 : #f7f7af
URxvt.color11                : #ffffaf

!blue.normal                : blue.bright.b - 8
!blue.bright                : xoria256.Statement.guifg
URxvt.color4                 : #87afd7
URxvt.color12                : #87afdf

!magenta.normal             : magenta.bright.rb - 8
!magenta.bright             : xoria256.Identifier.guifg
URxvt.color5                 : #d7afd7
URxvt.color13                : #dfafdf

!cyan.normal                : cyan.bright.gb - 8
!cyan.bright                : green.bright + blue.bright
URxvt.color6                 : #afd7d7
URxvt.color14                : #afdfdf

!white.normal               : white.bright.rgb - 8
!white.bright               : xoria256.Folded.guifg
URxvt.color7                 : #e6e6e6
URxvt.color15                : #eeeeee

!foreground                 : xoria256.Normal.guifg
!background                 : xoria256.Normal.guibg
!cursor                     : xoria256.Cursor.guibg
URxvt.foreground             : #d0d0d0
URxvt.background             : #1c1c1c
URxvt.cursorColor            : #ffaf00

!bold                       : xoria256.Type.guifg
!underline                  : xoria256.htmlValue.guifg
URxvt.colorBD                : #afafdf
URxvt.colorUL                : #dfdfaf

!black.normal               : xoria256.LineNr.guibg
!black.bright               : black.normal.rgb - 8
XTerm*vt100.color0          : #121212
XTerm*vt100.color8          : #0a0a0a

!red.normal                 : red.bright.r - 8
!red.bright                 : xoria256.Special.guifg
XTerm*vt100.color1          : #d78787
XTerm*vt100.color9          : #df8787

!green.normal               : green.bright.g - 8
!green.bright               : xoria256.PreProc.guifg
XTerm*vt100.color2          : #afd787
XTerm*vt100.color10         : #afdf87

!yellow.normal              : yellow.bright.rg - 8
!yellow.bright              : xoria256.Special.guifg
XTerm*vt100.color3          : #f7f7af
XTerm*vt100.color11         : #ffffaf

!blue.normal                : blue.bright.b - 8
!blue.bright                : xoria256.Statement.guifg
XTerm*vt100.color4          : #87afd7
XTerm*vt100.color12         : #87afdf

!magenta.normal             : magenta.bright.rb - 8
!magenta.bright             : xoria256.Identifier.guifg
XTerm*vt100.color5          : #d7afd7
XTerm*vt100.color13         : #dfafdf

!cyan.normal                : cyan.bright.gb - 8
!cyan.bright                : green.bright + blue.bright
XTerm*vt100.color6          : #afd7d7
XTerm*vt100.color14         : #afdfdf

!white.normal               : white.bright.rgb - 8
!white.bright               : xoria256.Folded.guifg
XTerm*vt100.color7          : #e6e6e6
XTerm*vt100.color15         : #eeeeee

!foreground                 : xoria256.Normal.guifg
!background                 : xoria256.Normal.guibg
!cursor                     : xoria256.Cursor.guibg
XTerm*vt100.foreground      : #d0d0d0
XTerm*vt100.background      : #1c1c1c
