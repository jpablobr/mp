! Appearance
#include "/home/jpablobr/.xrdb/themes/jpablobr.xrdb"

Xcursor.theme : Human

*background:            #1c1c1c
*foreground:            #657b83
*fading:                40
*fadeColor:             #1c1c1c
*cursorColor:           #93a1a1
*pointerColorBackground:#586e75
*pointerColorForeground:#93a1a1

Xft.autohint: 1
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

#ifdef COLOR
*customization: -color
#endif

! Man pages
*VT100.colorBDMode:     true
*VT100.colorBD:         red
*VT100.colorULMode:     true
*VT100.colorUL:         cyan

! Urxvt
URxvt.font: xft:proggynormal:normal:pixelsize=13
URxvt.boldFont: xft:proggynormal:normal:pixelsize=13
URxvt.letterSpace: -5
URxvt.termName : rxvt-256color
URxvt.scrollBar : off
URxvt.scrollstyle : plain
URxvt.iconFile : /usr/share/icons/gnome/22x22/apps/utilities-terminal.png
URxvt.geometry : 84Ã—24
URxvt.borderLess : False
URxvt.internalBorder : 0
URxvt.externalBorder : 0
URxvt.perl-lib : /home/jpablobr/.urxvt/perl
URxvt.perl-ext-common : default,matcher,searchable-scrollback,keyboard-select,url-select,clipboard,fullscreen
URxvt.keysym*M-u : perl:url-select:select_next
URxvt.keysym*M-Escape : perl:keyboard-select:activate
URxvt.keysym*M-q : perl:keyboard-select:search
URxvt.underlineURLs : true
URxvt.keysym*M-c : perl:clipboard:copy
URxvt.keysym*M-v : perl:clipboard:paste
URxvt.keysym*M-C-v : perl:clipboard:paste_escaped
URxvt.keysym*F11 : perl:fullscreen:switch
URxvt.copyCommand : xclip -i -selection clipboard
URxvt.pasteCommand : xclip -o -selection clipboard
URxvt.override-redirect : false
URxvt.matcher*button : 2
URxvt.urlLauncher : /usr/bin/chromium
URxvt.transparent : true
URxvt.shading : 30
URxvt.boldMode : on
URxvt.colorMode : on
URxvt.colorBDMode : on
URxvt.colorULMode : on
URxvt.underLine : on
URxvt.inheritPixmap : true

>85;95;0c! black dark/light
URxvt.color0:  #073642
URxvt.color8:  #1c1c1c
!! red dark/light
URxvt.color1:  #dc322f
URxvt.color9:  #cb4b16
!! green dark/light
URxvt.color2:  #859900
URxvt.color10: #586e75
!! yellow dark/light
URxvt.color3:  #b58900
URxvt.color11: #657b83
!! blue dark/light
URxvt.color4:  #268bd2
URxvt.color12: #839496
!! magenta dark/light
URxvt.color5:  #d33682
URxvt.color13: #6c71c4
!! cyan dark/light
URxvt.color6:  #2aa198
URxvt.color14: #93a1a1
!! white dark/light
URxvt.color7:  #eee8d5
URxvt.color15: #fdf6e3
URxvt.background: #000000
URxvt.foreground: #DDDDDD
URxvt.tintColor: #111111
! HLT Include, StatusLine, ErrorMsg
URxvt.colorIT: #87af5f
URxvt.colorBD: #d7d7d7
URxvt.colorUL: #87afd7
    
! Emacs
Emacs.menuBar: off
Emacs.verticalScrollBars: off
Emacs.toolBar: off
Emacs.BitmapIcon: on

! interaction
*vt100.metaSendsEscape : True
*vt100.saveLines       : 8192
*VT100.allowWindowOps: true
*VT100.allowTitleOps: true
*VT100.allowFontOps: true

! Xterm
XTerm*loginShell: true
XTerm*allowSendEvents: True
XTerm*sessionMgt: false
XTerm*vt100.colorMode       : True
XTerm*vt100.colorBDMode     : True
XTerm*vt100.colorULMode     : True
XTerm*vt100.showBlinkAsBold : True
XTerm*vt100.veryBoldColors  : 6
XTerm*Maximized             : true
XTerm*saveLines: 5120
XTerm*scrollKey: true
XTerm*scrollTtyOutput: false

